{
    "name": "nanhh/comment-like",
    "description": "comment and like comment in laravel",
    "keywords": [
        "nanhh",
        "comment-like"
    ],
    "homepage": "https://github.com/nanhh/comment-like",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nhat Anh",
            "email": "noly241161@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "erusev/parsedown": "^1.7",
        "illuminate/database": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/pagination": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/queue": "^9.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-honeypot": "^4.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Nanhh\\CommentLike\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nanhh\\CommentLike\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nanhh\\CommentLike\\ServiceProvider"
            ]
        }
    }
}
